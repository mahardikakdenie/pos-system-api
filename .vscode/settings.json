{
	"workbench.iconTheme": "vscode-icons",
	"editor.formatOnSave": false,
	"eslint.codeActionsOnSave.mode": "problems",
	"[vue]": {
		"editor.defaultFormatter": "dbaeumer.vscode-eslint"
	},
	"eslint.format.enable": true,
	"intelephense.environment.shortOpenTag": false,
	"intelephense.phpdoc.returnVoid": false,
	"php.suggest.basic": false,
	"php.validate.enable": false,
	"intelephense.diagnostics.typeErrors": false,
	"intelephense.diagnostics.undefinedFunctions": false,
	"intelephense.diagnostics.undefinedProperties": false,
	"intelephense.diagnostics.undefinedClassConstants": false,
	"intelephense.diagnostics.undefinedConstants": false,
	"intelephense.diagnostics.undefinedMethods": false,
	"intelephense.diagnostics.undefinedSymbols": false,
	"intelephense.diagnostics.undefinedTypes": false,
	"intelephense.diagnostics.undefinedVariables": false,
	"intelephense.diagnostics.unexpectedTokens": false,
	"intelephense.diagnostics.unusedSymbols": false,
	"html.format.contentUnformatted": "",
	"emmet.useInlineCompletions": true,
	"emmet.triggerExpansionOnTab": true,
	"emmet.showSuggestionsAsSnippets": true,
	"editor.snippetSuggestions": "top",
	"files.associations": {
		"*html": "html"
	},
	"[javascript]": {
		"editor.defaultFormatter": "dbaeumer.vscode-eslint"
	},
	"diffEditor.ignoreTrimWhitespace": false,
	"workbench.tree.renderIndentGuides": "always",
	"editor.guides.highlightActiveIndentation": "always",
	"vetur.format.options.tabSize": 4,
	"prettier.useTabs": true,
	"prettier.singleQuote": true,
	"prettier.endOfLine": "auto",
	"prettier.bracketSameLine": true,
	"prettier.jsxSingleQuote": true,
	"prettier.tabWidth": 4,
	"css.customData": [".vscode/css_custom_data.json"],
	"css.lint.unknownAtRules": "ignore",
	"css.lint.unknownProperties": "ignore",
	"less.lint.unknownProperties": "ignore",
	"scss.lint.unknownAtRules": "ignore",
	"scss.lint.unknownProperties": "ignore",
	"less.lint.unknownAtRules": "ignore",
	"files.insertFinalNewline": true,
	"files.trimFinalNewlines": true,
	"workbench.layoutControl.enabled": false,
	"gitlens.fileAnnotations.command": "blame",
	"gitlens.codeLens.includeSingleLineSymbols": true,
	"workbench.editor.showTabs": "single",
	"workbench.sideBar.location": "right",
	"workbench.settings.applyToAllProfiles": ["editor.fontFamily"],
	// "workbench.iconTheme": "fira-code-material-minimal",
	"editor.fontFamily": "Courier",
	"editor.fontLigatures": true,
	"editor.fontWeight": "900",
	"prisma.showPrismaDataPlatformNotification": false,
	"gitlens.hovers.currentLine.over": "line",
	"gitlens.launchpad.indicator.enabled": false,
	"[php]": {
		"editor.defaultFormatter": "bmewburn.vscode-intelephense-client"
	},
	"editor.unicodeHighlight.invisibleCharacters": false,
	"terminal.integrated.defaultProfile.windows": "Git Bash",
	"[typescript]": {
		"editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
	},
	"workbench.colorTheme": "JetBrains Darcula Theme",
	"github.copilot.nextEditSuggestions.enabled": true,
	"github.copilot.enable": {
		"*": false,
		"plaintext": false,
		"markdown": false,
		"scminput": false,
		"typescriptreact": false
	}
}
